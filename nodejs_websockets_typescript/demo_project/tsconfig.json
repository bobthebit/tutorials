{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist/server",
      "strict": true,
      "sourceMap": true,
      "typeRoots": [
          "node_modules/@types"
      ],
      "baseUrl": "src",
      "paths": {
        "@blub/*" : ["blub/*"]
      }
  },
  "include": [
  "src/**/*.ts"
],
  "exclude": [
      "dist",
      "node_modules",
      ".vscode"
  ]
}